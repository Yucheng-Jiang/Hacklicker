<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#D3D3D3">


    <ImageButton
        android:id="@+id/create_room"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:background="@drawable/rounded_button"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/create_room"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="72dp"
        tools:layout_editor_absoluteY="204dp" />

    <ImageButton
        android:id="@+id/question_history"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:background="@drawable/rounded_button"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/question_history"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="72dp"
        tools:layout_editor_absoluteY="427dp" />

    <ImageButton
        android:id="@+id/join_room"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:background="@drawable/rounded_button"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/join_room"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="252dp"
        tools:layout_editor_absoluteY="204dp" />

    <ImageButton
        android:id="@+id/export"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:background="@drawable/rounded_button"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/export"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="252dp"
        tools:layout_editor_absoluteY="427dp" />

    <ImageButton
        android:id="@+id/settings"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#D3D3D3"
        app:srcCompat="@drawable/settings"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="342dp"
        tools:layout_editor_absoluteY="16dp" />


</androidx.constraintlayout.widget.ConstraintLayout>